<Window x:Class="WpfTetris.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="WPF Tetris" KeyDown="keyDown" KeyUp="keyUp">
    <Canvas Name="mainCanvas" SizeChanged="resize">
        <Image Stretch="Uniform" Name="backgroundImage" Source="/WpfTetris;component/Resources/background.png" Height="500" VerticalAlignment="Bottom" />
        <Rectangle Canvas.Left="148" Canvas.Top="110" Height="273" Name="playingField" Width="455" Fill="#40000000" />
    </Canvas>
</Window>
